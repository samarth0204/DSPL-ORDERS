{
  "compilerOptions": {
  "target": "ES2020",
  "module": "commonjs",
  "outDir": "dist",
  "rootDir": "src",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "resolveJsonModule": true,
  "allowJs": true
},
"include": ["src", "src/generated/prisma"],
"exclude": ["node_modules", "dist"]
}
